<Window x:Class="Emulate.Administration"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Emulate"
        mc:Ignorable="d"
        Title="Administration" Height="300" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button x:Name="btnItems" Click="btnItems_Click" Content="Items" Grid.Row="0" Grid.Column="0" />
        <Button x:Name="btnClasse" Click="btnClasse_Click" Content="Classe"  Grid.Row="0" Grid.Column="1" />
        <Button x:Name="btnPlayer" Click="btnPlayer_Click" Content="Player"  Grid.Row="0" Grid.Column="2" />
        <Button x:Name="btnBoss" Click="btnBoss_Click" Content="Boss"  Grid.Row="1" Grid.Column="1" />
        <Button x:Name="btnDonjon" Click="btnDonjon_Click" Content="Donjon"  Grid.Row="1" Grid.Column="2" />
        <Button x:Name="btnPartie" Click="btnPartie_Click" Content="Partie"  Grid.Row="2" Grid.Column="0" />
    </Grid>
</Window>
